 Project\\Assignment\\GroupProject\\Frontend\\src\\components\\Student\\Attendance\\AttendanceDetail.js",["310","311","312","313"],"import React from 'react';\r\nimport PropTypes from 'prop-types';\r\n\r\nimport AttendanceDays from './AttendanceDays';\r\n\r\nfunction AttendanceDetail(props) {\r\n  const { module_name, attendance_status } = props;\r\n\r\n  let count = 0;\r\n  return (\r\n    <div className=\"class-attendance-item\">\r\n      <div className=\"class-detail\">\r\n        <div className=\"class-heading\">{module_name}</div>\r\n        <div className=\"days-container\">\r\n          <div className=\"label\">Days:</div>\r\n\r\n          <div className=\"day-marks\">\r\n            {attendance_status.map((item, index) => (\r\n              // eslint-disable-next-line no-plusplus\r\n              <AttendanceDays key={++count} present={!!item} number={index} />))}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n  );\r\n}\r\n\r\nAttendanceDetail.propTypes = {\r\n  module_name: PropTypes.string.isRequired,\r\n  attendance_status: PropTypes.array.isRequired,\r\n};\r\n\r\nexport default AttendanceDetail;\r\n","D:\\Bsc\\L5\\Group Project\\Assignment\\GroupProject\\Frontend\\src\\components\\Student\\Attendance\\ActiveAttendance.js",["314","315"],"import React from 'react';\r\nimport PropTypes from 'prop-types';\r\n\r\nconst ActiveAttendance = (props) => {\r\n  const {\r\n    module_name, week, time, click,\r\n  } = props;\r\n\r\n  return (\r\n    <div className=\"class-attendance-item\">\r\n\r\n      <div className=\"heading\">Current Class: Attendance Required!</div>\r\n\r\n      <div className=\"class-detail class-detail-active\">\r\n        <div className=\"class-heading\">{module_name}</div>\r\n        <p style={{ marginTop: '10px' }}>\r\n          <strong>\r\n            Week\r\n            {` ${week}`}\r\n          </strong>\r\n          {' '}\r\n        </p>\r\n        <div className=\"days-container\" style={{ marginTop: '0' }}>\