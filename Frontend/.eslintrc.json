{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style":"off",
        "max-len":"warn",
        "no-unused-expressions":"warn",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-use-before-define":"off",
        "camelcase":"warn",
        "radix":"warn",
        "import/prefer-default-export":"warn",
        "react/forbid-prop-types":"warn"
    }
}
